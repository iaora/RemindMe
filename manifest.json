{
  "manifest_version": 2,

  "name": "RemindMe",
  "description": "This extension pushes a notification to remmind you to do things.",
  "version": "1.0",

  "permissions":[
    "notifications"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "RemindMe!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  "web_accessible_resources":[
    "icon.png"
  ]
}
